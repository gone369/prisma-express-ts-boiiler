{
  "name": "prisma-express-ts-boiler",
  "description": "boilerplate for express prisma backend node.js app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Xun Chen <xunchen369@gmail.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --no-notify --respawn --transpile-only src/server",
    "start": "ts-node -r tsconfig-paths/register --transpile-only src/server",
    "clean": "rm -rf dist",
    "migrate": "prisma migrate dev --preview-feature",
    "generate": "npm -s run generate:prisma",
    "generate:prisma": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^2.18.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "prisma": "^2.18.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  }
}
